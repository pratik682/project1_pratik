<h1>Listing Users</h1>
 
<table>
  <tr>
    <th>Name</th>
    <th>email</th>
    <th></th>
    <th></th>
  </tr>
 
<% @user.each do |u| %>
  <tr>
    <td><%= u.name %></td>
    <td><%= u.email %></td>
    <td><%= link_to 'Show', user_path(u) %></td>
    <td><%= link_to 'Edit', edit_user_path(u) %></td>
    <td><%= link_to 'Delete', user_path(u),:method => :delete %></td>
  </tr>
<% end %>
</table>
 
<br />
 
<%= link_to 'Create A User',new_user_path%>
