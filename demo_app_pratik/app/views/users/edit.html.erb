<%= form_for @user do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<br/>

<%= f.label :email, "Enter email" %>
<%= f.text_field :email %>
<br/>

<%= f.label :first_name, "Enter First Name" %>
<%= f.text_field :first_name %>
<br/>

<%= f.label :last_name, "Enter Last Name" %>
<%= f.text_field :last_name %>
<br/>

<%= f.label :full_name, "Enter Full Name" %>
<%= f.text_field :full_name %>
<br/>

<%= f.label :contact_no %>
<%= f.text_field :contact_no %>
<br/>

<%= f.label :user_type %>
<%= f.text_field :user_type %>
<br/>



<%= f.submit "UPDATE" %>
<%end%>
